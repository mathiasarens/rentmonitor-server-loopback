    {
      "type": "node",
      "request": "launch",
      "name": "Selected Test",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--colors",
        "${workspaceFolder}/dist/__tests__/**/${fileBasenameNoExtension}.js"
      ],
      "env": {
        "NODE_ENV": "test"
      },
      "preLaunchTask": "npm: build",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector"
    }
  ]
}
