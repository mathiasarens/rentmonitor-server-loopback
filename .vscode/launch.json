      "args": [
        "--colors",
        "${workspaceFolder}/dist/__tests__/**/tenant.controller.acceptance*.js"
      ],
      "env": {
        "NODE_ENV": "test"
      },
      "preLaunchTask": "npm: build",
      "console": "integratedTerminal",
