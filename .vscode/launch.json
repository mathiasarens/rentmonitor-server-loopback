      "env": {
        "NODE_ENV": "test"
      },
      "preLaunchTask": "npm: build",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Tenant Controller Acceptance Tests",
