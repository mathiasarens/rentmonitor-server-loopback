{
  "name": "rentmonitor-server",
  "version": "2.1.0",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "TZ=UTC lb-mocha --allow-console-logs \"dist/src/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/src/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback",
    "start-serverless": "npm run build && serverless offline",
    "migrate": "node ./dist/src/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/src/migrate -- --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/src/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "TZ=UTC PORT=3001 node -r source-map-support/register dist",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.0",
    "@loopback/authorization": "^0.12.0",
    "@loopback/boot": "^5.0.0",
    "@loopback/context": "^5.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/openapi-v3": "^8.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/rest-explorer": "^5.0.0",
    "@loopback/security": "^0.8.0",
    "@loopback/service-proxy": "^5.0.0",
    "aws-serverless-express": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "loopback-connector-postgresql": "^5.5.0",
    "node-fints": "^0.5.1",
    "serverless": "^3.2.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/repository-json-schema": "^6.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.20.50",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-mocha": "10.0.4",
    "mocha": "^10.0.0",
    "serverless-offline": "^8.4.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.4"
  }
}
