{
  "name": "rentmonitor-server",
  "version": "2.4.0",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "TZ=UTC lb-mocha --allow-console-logs \"dist/src/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/src/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback",
    "start-serverless": "npm run build && serverless offline",
    "migrate": "node ./dist/src/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/src/migrate -- --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/src/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "TZ=UTC PORT=3001 node -r source-map-support/register dist",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.1.5",
    "@loopback/authorization": "^0.14.5",
    "@loopback/boot": "^6.1.5",
    "@loopback/context": "^6.1.5",
    "@loopback/core": "^5.1.5",
    "@loopback/openapi-v3": "^9.1.5",
    "@loopback/repository": "^6.1.5",
    "@loopback/rest": "^13.1.5",
    "@loopback/rest-explorer": "^6.1.5",
    "@loopback/security": "^0.10.5",
    "@loopback/service-proxy": "^6.1.5",
    "aws-serverless-express": "^3.4.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "loopback-connector-postgresql": "^7.0.2",
    "@mathiasarens/fints": "^0.5.1",
    "serverless": "^3.25.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.5",
    "@loopback/eslint-config": "^14.0.5",
    "@loopback/repository-json-schema": "^7.2.1",
    "@loopback/testlab": "^6.1.5",
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^16.18.70",
    "@types/node-fetch": "^2.6.10",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.2.0",
    "serverless-offline": "^13.3.2",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
