{
  "name": "rentmonitor-server",
  "version": "1.0.13",
  "description": "Monitor the payments of your tenants. Did every renter pay on time?",
  "keywords": [
    "rent",
    "check",
    "tenant",
    "payments",
    "fints-support"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "TZ=UTC lb-mocha --allow-console-logs \"dist/src/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/src/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t rentmonitor-server-loopback .",
    "docker:run": "docker run -p 3000:3000 -d rentmonitor-server-loopback",
    "migrate": "node ./dist/src/migrate",
    "migrate:testdb": "NODE_ENV=test node ./dist/src/migrate -- --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/src/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "TZ=UTC PORT=3001 node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathiasarens/rentmonitor-server-loopback.git"
  },
  "author": "Mathias Arens",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.0.1",
    "@loopback/authorization": "^0.10.1",
    "@loopback/boot": "^4.0.1",
    "@loopback/cli": "^3.0.1",
    "@loopback/context": "^4.0.1",
    "@loopback/openapi-v3": "^7.0.1",
    "@loopback/repository": "^4.0.1",
    "@loopback/rest": "^11.0.1",
    "@loopback/rest-explorer": "^4.0.1",
    "@loopback/security": "^0.6.1",
    "@loopback/service-proxy": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.3",
    "isemail": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "loopback-connector-postgresql": "^5.5.0",
    "node-fints": "^0.5.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.1",
    "@loopback/eslint-config": "^12.0.1",
    "@loopback/repository-json-schema": "^5.0.1",
    "@loopback/testlab": "^4.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^9.0.0",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "source-map-support": "^0.5.21",
    "typescript": "~4.5.4"
  }
}
